generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Peserta {
  id          String   @id @default(uuid())
  fullName    String
  asalSekolah String
  tglLahir    DateTime
  usia        String
}

model User {
  id           String  @id @default(uuid())
  userName     String
  email        String  @unique
  password     String
  role         String
  accessToken  String? @unique
  refreshToken String? @unique
}
